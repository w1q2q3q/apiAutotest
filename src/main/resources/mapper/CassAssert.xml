<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Mapper.CassAssertMapper">
    <resultMap id="CassAssert" type="com.example.demo.Entity.CassAssert">
        <id property="aid" column="aid" javaType="java.lang.Integer"/>
        <result property="aname" column="aname" javaType="java.lang.String"/>
        <result property="aparamter" column="aparamter" javaType="java.lang.String"/>
        <result property="acassid" column="acassid" javaType="java.lang.String"/>
        <!--<association property="TestCass"-->
                     <!--resultMap="com.example.demo.Mapper.TestCassMapper"></association>-->
    </resultMap>
    <!-- 批量新增的时候，注意接收的对象是一个列表，需要在动态拼接sql的时候遍历参数集合对象 -->
    <insert id="addAssert" parameterType="java.util.List">
        Insert into cassassert (name,paramter,cassid) values
        <!-- 注意foreach的语法,集合直接写list字符串，用item表示当前遍历的对象，结果集合通过使用逗号分隔使用separator属性 -->
        <foreach collection="list" item="item" separator=",">
            (#{item.name},#{item.paramter},#{item.cassid})
        </foreach>
    </insert>

    <insert id="addOneAssert" parameterType="com.example.demo.Entity.CassAssert">
        Insert into cassassert
        (name,paramter,cassid) values(#{item.name},#{item.paramter},#{item.cassid})
    </insert>

</mapper>